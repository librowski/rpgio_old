{
    "files.exclude": {
        "**/logs": true,
        "**/*.log": true,
        "**/npm-debug.log*": true,
        "**/yarn-debug.log*": true,
        "**/yarn-error.log*": true,
        "**/report.[0-9]*.[0-9]*.[0-9]*.[0-9]*.json": true,
        "**/lib-cov": true,
        "**/coverage": true,
        "**/*.lcov": true,
        "**/.nyc_output": true,
        "**/node_modules/": true,
        "**/.eslintcache": true,
        "**/.yarn-integrity": true,
        "**/.env": true,
        "**/.env.test": true,
        "**/.DS_*": true,
        "**/*.backup.*": true,
        "**/*.back.*": true,
        "**/*.sublime*": true,
        "**/.scannerwork": true,
        "**/public": true,
        "**/reports": true,
        "**/.vscode": true
    },
    "typescript.preferences.importModuleSpecifier": "auto",
    "typescript.preferences.quoteStyle": "single",
    "typescript.preferences.useAliasesForRenames": false,
}